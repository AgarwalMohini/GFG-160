class Solution:
	def pushZerosToEnd(self,arr):
    	# code here
    	l1=[]
    	l2=[]
    	for i in range(len(arr)):
    	    if arr[i]==0:
    	        l1.append(arr[i])
    	    else:
    	        l2.append(arr[i])
    	l3=l2+l1
    	for i in range(len(arr)):
    	    arr[i]=l3[i]
    	return arr